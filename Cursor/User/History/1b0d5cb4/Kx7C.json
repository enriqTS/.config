{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DynamoDBOfertasTableAccess",
      "Effect": "Allow",
      "Action": [
        "dynamodb:GetItem"
      ],
      "Resource": "arn:aws:dynamodb:us-east-1:700308877414:table/ofertas"
    },
    {
      "Sid": "DynamoDBApiErrorsTableAccess",
      "Effect": "Allow",
      "Action": [
        "dynamodb:PutItem"
      ],
      "Resource": "arn:aws:dynamodb:us-east-1:700308877414:table/api_errors"
    },
    {
      "Sid": "CloudWatchLogsAccess",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:us-east-1:700308877414:log-group:/aws/lambda/verificar_tipo_veiculo_permitido-679fg:*"
    }
  ]
}
